{
  "version": 2,
  "builds": [
    {
      "src": "dist/shell/index.html",
      "use": "@vercel/static",
      "config": {
        "outputDirectory": "dist/shell"
      }
    },
    {
      "src": "dist/customers-mfe/remoteEntry.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/customers-mfe/(.*)",
      "dest": "/customers-mfe/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
